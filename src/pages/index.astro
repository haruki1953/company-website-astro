---
import Layout from "@/layouts/Layout.astro";
import HeaderLarge from "@/components/HeaderLarge.astro";
import { webName } from "@/config";

import headerImg1 from "@/assets/img/demo/1.jpg";
import headerImg2 from "@/assets/img/demo/2.jpg";
import headerImg3 from "@/assets/img/demo/3.jpg";
import headerImg4 from "@/assets/img/demo/4.jpg";
import headerImg5 from "@/assets/img/demo/5.jpg";
import headerImg6 from "@/assets/img/demo/7.jpg";
---

<Layout title={webName}>
  <HeaderLarge title={webName} buttonLink="/page/aboutus" img={headerImg1}>
    2024年成立的服装设计公司。创办人为干早爱音。
  </HeaderLarge>

  <HeaderLarge
    title="首席执行官"
    buttonLink="/page/aboutus"
    img={headerImg2}
    aos
  >
    千早爱音
  </HeaderLarge>

  <HeaderLarge
    title="首席裁缝师"
    buttonLink="/page/aboutus"
    img={headerImg3}
    aos
  >
    要乐奈
  </HeaderLarge>

  <HeaderLarge title="形象大使" buttonLink="/page/aboutus" img={headerImg4} aos>
    高松灯
  </HeaderLarge>

  <HeaderLarge
    title="首席饲养员"
    buttonLink="/page/aboutus"
    img={headerImg5}
    aos
  >
    椎名立希
  </HeaderLarge>

  <HeaderLarge
    title="首席执行官"
    buttonLink="/page/aboutus"
    img={headerImg6}
    aos
  >
    长崎爽世
  </HeaderLarge>
</Layout>
